<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="654.0" prefWidth="1038.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bakalarskyprojekt.controllers.ConsoleComponentController">
   <children>
      <BorderPane prefHeight="510.0" prefWidth="1038.0" HBox.hgrow="ALWAYS">
         <center>
            <VBox BorderPane.alignment="CENTER">
            <children>
                  <HBox style="-fx-background-color: #285F85;">
                     <children>
                        <HBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
                           <children>
                  <Label text="Test - Output" textFill="WHITE">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin></Label>
                           </children>
                        </HBox>
                        <ToggleButton fx:id="btnSaveConsoleOutput" mnemonicParsing="false" onAction="#saveOutputOnAction" style="-fx-background-radius: 0;" text="Save output">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </ToggleButton>
                     </children>
                  </HBox>
            <AnchorPane fx:id="anchp" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
            </children>
            </VBox>
         </center>
         <bottom>
            <VBox alignment="TOP_CENTER" prefHeight="54.0" prefWidth="480.0" BorderPane.alignment="CENTER">
               <children>
                  <Pane prefHeight="31.0" prefWidth="469.0" style="-fx-background-color: #285F85;">
      <children>
      <Label layoutX="14.0" layoutY="7.0" text="Input" textFill="WHITE" />
      </children>
      </Pane>
                  <HBox>
                     <children>
                        <TextField fx:id="commandField" prefHeight="25.0" prefWidth="67.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="executeBtn" mnemonicParsing="false" onAction="#exteClick" style="-fx-background-radius: 0;" text="Execute" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </bottom>
      </BorderPane>
   </children>
</HBox>
